.panel-heading-custom {
    background: #eeeeee;
    border-bottom: 0 none;
    border-radius: 0; 
    position: relative;
    z-index: 3;
    padding:12px 0px 5px 0px;
    text-align:left;
    color:#9d9b9b;
}
.panel-title-custom {
    background: transparent none repeat scroll 0 0;
    border-bottom: 0 none;
    border-radius: 0;
    position: relative;
    z-index: 3;
    font-family: 'Montserrat';
    text-transform: uppercase;
    display: inline-block;
    letter-spacing: 0.02em;
    font-size: 12px;
    font-weight: 600;
    margin: 0;
    padding-left: 10px;
    line-height: normal;
    overflow: hidden;
    text-overflow: ellipsis;
    -webkit-text-stroke: 0px;
    filter: alpha(opacity=40);
    -webkit-transition: opacity 0.3s ease;
    transition: opacity 0.3s ease;
}

.panel-title1 {
    font-family: 'Montserrat';
    text-transform: uppercase;
    display: inline-block;
    letter-spacing: 0.02em;
    font-size: 15px;
    font-weight: 600;
    line-height: normal;
    overflow: hidden;
    text-overflow: ellipsis;
    -webkit-text-stroke: 0px;
    filter: alpha(opacity=40);
    -webkit-transition: opacity 0.3s ease;
    transition: opacity 0.3s ease;
    padding: 20px 0px 15px 0px;
}

.alert .close {
    width: 10px;
    height: 9px;
    position: relative;
    opacity: 0.8;
    top: 0px !important;
    float: right;
    margin-left: 20px;
    font-size: 13px;
    color: #933432;
}
